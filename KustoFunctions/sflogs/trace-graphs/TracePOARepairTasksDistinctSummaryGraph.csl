.create-or-alter function with (docstring = "[T:string] where T=table name. function to count 'POA-' patch orchestration repair tasks summary in timechart'",folder = "sflogs/trace-graphs") 
    TracePOARepairTasksDistinctSummaryGraph(T:string) {
        TracePOARepairTasks(T)
        | summarize max(repairTaskCount) by bin(Timestamp,1m), repairTaskCount
        | render timechart 
}