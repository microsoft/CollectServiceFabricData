.create-or-alter function with (docstring = "[t:string, paritionId:string]
        where t=table name and paritionId=Parition Id. 
        Return begin TimeStamp of partition.", folder = "rc-perf")
    GetPartitionStartTime(t:string, partitionId:string) {
    let timeRange = table(t)
    | where Type has partitionId
    | summarize StartTime = min(Timestamp);
    toscalar(timeRange | project StartTime);
}
